<?xml version="1.0" encoding="utf-8"?>
<animated-vector
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:aapt="http://schemas.android.com/aapt"
  android:drawable="@drawable/viv_vd_pathmorph_digits_zero">

  <target android:name="iconPath">
    <aapt:attr name="android:animation">
      <objectAnimator
        android:duration="@integer/viv_animation_duration"
        android:propertyName="pathData"
        android:valueFrom="@string/viv_path_eight"
        android:valueTo="@string/viv_path_three"
        android:valueType="pathType"/>
    </aapt:attr>
  </target>

</animated-vector>
